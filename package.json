{
  "name": "ts-serverless",
  "private": false,
  "version": "1.0.1",
  "description": "",
  "scripts": {
    "ci": "npm run lint && npm run test",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:sonar": "jest --passWithNoTests --coverage --no-watch --coverage -c jest.config.js --testResultsProcessor jest-sonar-reporter",
    "lint": "eslint --ext .ts .",
    "lint:fix": "eslint --ext .ts --fix .",
    "sonar:scan": "node sonarqube-scanner.js",
    "sonar:all": "npm run test:sonar && npm run sonar:scan",
    "generate": "node _templates/generate.ts",
    "prepare": "husky install"
  },
  "engines": {
    "node": ">=20.10.0"
  },
  "lint-staged": {
    "**/*.js": "eslint --fix",
    "**/*": "prettier --write --ignore-unknown"
  },
  "devDependencies": {
    "@jest/transform": "^29.7.0",
    "@serverless/typescript": "^3.30.1",
    "@types/aws-lambda": "^8.10.129",
    "@types/jest": "^29.5.10",
    "@types/lambda-log": "^3.0.3",
    "@types/node": "^20.10.0",
    "@types/serverless": "^3.12.18",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "aws-lambda": "^1.0.7",
    "aws-lambda-mock-context": "^3.2.1",
    "aws-sdk": "^2.1504.0",
    "axios": "^1.6.2",
    "crypto-js": "^4.2.0",
    "dotenv": "^16.3.1",
    "esbuild": "^0.17.19",
    "eslint": "^8.54.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-config-standard-with-typescript": "^40.0.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-n": "^16.3.1",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-promise": "^6.1.1",
    "gitmoji-cli": "^9.0.0",
    "glob": "^10.3.10",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "jest-junit": "^16.0.0",
    "jest-sonar-reporter": "^2.0.0",
    "jsonwebtoken": "^9.0.2",
    "lambda-log": "^3.1.0",
    "lint-staged": "^15.1.0",
    "nanoid": "^5.0.3",
    "node-loader": "^2.0.0",
    "nodemailer": "^6.9.7",
    "nodemailer-mock": "^2.0.2",
    "prettier": "^3.1.0",
    "serverless": "^3.38.0",
    "serverless-deployment-bucket": "^1.6.0",
    "serverless-domain-manager": "^7.2.1",
    "serverless-esbuild": "^1.46.0",
    "serverless-layers": "^2.7.0",
    "serverless-offline": "^12.0.4",
    "serverless-prune-plugin": "^2.0.2",
    "serverless-webpack": "^5.13.0",
    "sonarqube-scanner": "^3.3.0",
    "ts-jest": "^29.1.1",
    "ts-loader": "^9.5.1",
    "tsconfig-paths-webpack-plugin": "^4.1.0",
    "tsx": "^4.6.0",
    "typescript": "^5.3.2"
  }
